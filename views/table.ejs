<%- include('layouts/sidebar', { navigationLinks }) %>
            <div class="container-fluid pt-4 px-4">
                <div class="bg-dark text-center rounded p-4">
                    <div class="d-flex align-items-center justify-content-between mb-4"> 
                        <h6 class="mb-0 text-light"><%= active %></h6>
                        <a href="">Show All</a>
                    </div>
                    <div class="table-responsive">
                        <table class="table text-start table-dark align-middle table-bordered table-hover mb-0">
                            <thead>
                                <tr class="text-light">
                                    <th scope="col">Name</th>
                                    <th scope="col">Description</th>
                                    <th scope="col">Parameter</th>
                                    <th scope="col">Type</th>
                                    <th scope="col">Status</th>
                                    <th scope="col">Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% plugins.forEach(plugin => { %>
                                    <tr>
                                        <td><%- plugin.name %></td>
                                        <td><%- plugin.description %></td>
                                        <td><%- plugin.parameter %></td>
                                        <td><%- plugin.type %></td>
                                        <td><%- plugin.status %></td>
                                        <td><a class="btn btn-sm btn-primary" href="<%- plugin.action + apikey %>">Detail</a></td>
                                    </tr>
                                    <% }); %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
<%- include('layouts/footer')  %> 